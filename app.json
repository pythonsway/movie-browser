{
  "expo": {
    "name": "Movie Browser",
    "slug": "movie-browser",
    "description": "View information about any movies.",
    "owner": "pythonsway",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "githubUrl": "https://github.com/pythonsway/movie-browser",
    "orientation": "portrait",
    "primaryColor": "#ffa500",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffa500"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "it.pythonsway.moviebrowser",
      "supportsTablet": true,
      "icon": "./assets/icon_ios.png"
    },
    "android": {
      "package": "it.pythonsway.moviebrowser"
    }
  }
}